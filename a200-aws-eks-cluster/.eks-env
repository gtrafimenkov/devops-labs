# Environment variables in case AWS EKS cluster is used.
# Apply settings with `. .eks-env`
# Fetch kubeconfig with `make k8s-eks-get-config`

export AWS_REGION=eu-west-1
export EKS_CLUSTER=test-eks1
export KUBECONFIG=~/.kube/eks.$EKS_CLUSTER
